
<div class="row">

  {{#each items}}

    <div class="col-sm-6 col-md-4 colpad">
      <div class="thumbnail">
        <img src={{imagePath}} class="img-responsive">
        <div class="caption">
          <h3>{{name}}</h3>
          <p class="description">{{description}}</p>
          <div class="clearfix">
              <div class="price pull-left"> {{price}}</div>
              <a href="#" class="btn btn-success pull-right" role="button">Button</a>
          </div>
        </div>
      </div>
    </div>

  {{/each}}
  
</div>


  
